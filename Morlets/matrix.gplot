#set term postscript enhanced colour font 'Helv, 10'
#set output 'Result.ps'
set term x11
#set term pngcairo enhanced font 'arial,12'
#set output 'Result.png'
set pm3d map

set logscale z 10
set logscale y 2

#shows where the tics start
set ticslevel 0

set xlabel "time (s)"
set ylabel "Frequency (Hz)"
set title "Time-Frequency graph of x(t)"

#set yrange [0.2:4]
splot "DATA.log" matrix nonuniform with pm3d t ''
pause -1 "Hit Return to continue"
